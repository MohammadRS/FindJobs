@using FindJobs.Domain.Dtos
@model IEnumerable<ApplicantEducationDto>



<table class="applicantProfiletable educationTable">

    <tbody>
        @if (Model.Count() < 1)
        {
            <tr>
                <td colspan="6">@Res.ApplicantProfile.ThereIsNoEducation</td>

            </tr>
        }
        @foreach (var item in Model)
        {
            <tr>
                <td><b>@item.EducationLevel</b></td>
                <td>@item.CourseName</td>
                <td>@item.InstituteName</td>
                @if(item.StartEducation !=null){
                    <td>@item.StartEducation.Value.ToString("yyyy-MM")</td>
                }else{
                    <td></td>
                }
                @if(item.EndEducation !=null){
                    <td>@item.EndEducation.Value.ToString("yyyy-MM")</td>
                }else{
                    <td></td>
                }
                <td>
                    <img onclick="getEntity(@item.Id,'GetApplicantEducation','ApplicantEducationAddUpdate','ApplicantEducationModal')" class="ApplicantProfileIcon" src="data:image/png;base64,">
                    <img onclick="deleteEntity(@item.Id,'RemoveApplicantEducation','Education','ApplicantEducationListDiv','ApplicantEducationModal')" class="ApplicantProfileIcon" src="data:image/png;base64,">
                </td>
            </tr>
        }

        
    </tbody>
</table>