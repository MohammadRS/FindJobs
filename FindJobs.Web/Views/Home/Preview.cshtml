@using FindJobs.Domain.Dtos
@using FindJobs.Domain.Enums;
@using FindJobs.Web.Controllers;
@model OfferViewModel

@section Css{
    <link rel="stylesheet" href="/dcss/internal/company-details-review/@ViewData["ColorTeal"]~@ViewData["ColorAqua"]" />
}
@section Script{
    <script src="~/js/internal/publicJs.js" async defer></script>
}


<div class="container">


    <div class="split-flex">
        <div id="Review" class="mg-top right-table-width">
            <div>
                <details class="detailsbox" open>
                    <summary>@Res.CompanyOffer.Review</summary>
                    <table class="table">
                        <tbody>
                            <tr>
                                <td>
                                    <div>
                                        <h4>
                                            <span id="jobTitleSpan">@Model.offerDto.JobTitle</span>/<span id="companyNameSpan">@Model.offerDto.CompanyDto.Name</span>
                                        </h4>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <table class="table">
                        <tbody>
                            <tr class="justify-between">
                                <td>
                                    <div class="center-text">
                                        <img style="height:50px;width:50px;" src="data:image/png;base64,">
                                        <div>
                                            <div>
                                                <div class="image-offer-text-title">  @Res.CompanyOffer.AddressOfWork </div>
                                            </div>
                                            <div id="addressOfWorkP"> @Model.offerDto.WorkPlaceAddress </div>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="center-text">
                                        <img style="height:50px;width:50px" src="data:image/png;base64,">                                                <div>
                                            <div>
                                                <div class="image-offer-text-title">   @Res.CompanyOffer.WageConditions </div>
                                            </div>
                                            @Res.CompanyOffer.BasicSalary : <span id="baseOfSalarySapn"> @Model.offerDto.BasicSalary</span>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="center-text">
                                        <img style="height:50px;width:50px" src="data:image/png;base64,">                                                <div>
                                            <div>
                                                <div>   @Res.CompanyOffer.TypeOfContract </div>
                                            </div>
                                            @{
                                                var contracts = HomeController.GetTypeOfContractList(Model.offerDto);
                                            }
                                            @foreach (var item in contracts)
                                            {
                                                <span>@item</span>
                                                if (contracts.Last() != item)
                                                {
                                                    <span>,</span>
                                                }
                                            }

                                        </div>
                                    </div>
                                </td>

                            </tr>
                        </tbody>
                    </table>



                    <table class="table">


                        <tbody>
                            <tr>
                                <td>
                                    <div>
                                        <h3>@Res.CompanyOffer.JobDescription</h3>

                                    </div>
                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <p>@Model.offerDto.JobDescription</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div>
                                        <h3> @Res.CompanyOffer.Benifits</h3>

                                    </div>
                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <p>@Model.offerDto.Benifits</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div>
                                        <h3>   @Res.CompanyOffer.RequirementsPerEmployee</h3>

                                    </div>
                                </td>
                            </tr>
                            <tr>


                                <td>
                                    <div>
                                        <h3> @Res.CompanyOffer.Education</h3>
                                    </div>
                                </td>


                                <td>
                                    @{
                                        var educationList = HomeController.GetEducationList(Model.offerDto);
                                    }
                                    @foreach (var item in educationList)
                                    {
                                        <span>@item</span>
                                        if (educationList.Last() != item)
                                        {
                                            <span>,</span>
                                        }
                                    }

                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <div>
                                        <h3> @Res.CompanyOffer.LanguageSkills</h3>

                                    </div>
                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <div>
                                        <h3>  @Res.CompanyOffer.RequiredLanguageSkills</h3>

                                    </div>
                                </td>
                                <td>
                                    @{
                                        var offdto = Model.offerDto.OfferLanguageDtos.Where(x => x.LanguageLevel == (int)OfferLanguageType.Required);
                                    }
                                    @foreach (var item in offdto)
                                    {
                                        <span>@item.LanguageDto.Name </span>
                                        if (offdto.Last() != item)
                                        {
                                            <span>,</span>
                                        }

                                    }
                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <div>
                                        <h3> @Res.CompanyOffer.OptionalLanguageSkills</h3>

                                    </div>
                                </td>
                                <td>
                                    @{
                                        var latdto = Model.offerDto.OfferLanguageDtos.Where(x => x.LanguageLevel == (int)OfferLanguageType.Optional);
                                    }
                                    @foreach (var item in latdto)
                                    {
                                        <span>@item.LanguageDto.Name</span>
                                        if (latdto.Last() != item)
                                        {
                                            <span>,</span>
                                        }
                                    }
                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <div>
                                        <h3> @Res.CompanyOffer.OtherKnowledge</h3>

                                    </div>
                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <div>
                                        <h3> @Res.CompanyOffer.RequiredJuniorKnowledge</h3>

                                    </div>
                                </td>
                                <td>
                                    @{
                                        var kndto = Model.offerDto.OfferKnowledgeDtos.Where(x => x.KnowledgeLevel == (int)KnowledgeLevel.RequiredJuniorKnowledge);
                                    }
                                    @foreach (var item in kndto)
                                    {
                                        <span>@item.KnowledgeDto.Name</span>
                                        if (kndto.Last() != item)
                                        {
                                            <span>,</span>
                                        }
                                    }


                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <div>
                                        <h3> @Res.CompanyOffer.RequiredSeniorknowledge</h3>

                                    </div>
                                </td>
                                <td>
                                    @{
                                        var knsdto = Model.offerDto.OfferKnowledgeDtos.Where(x => x.KnowledgeLevel == (int)KnowledgeLevel.RequiredSeniorknowledge);
                                    }
                                    @foreach (var item in knsdto)
                                    {
                                        <span>@item.KnowledgeDto.Name</span>
                                        if (knsdto.Last() != item)
                                        {
                                            <span>,</span>
                                        }
                                    }

                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <div>
                                        <h3>  @Res.CompanyOffer.Advantage_OptionalKnowledge</h3>

                                    </div>
                                </td>
                                <td>
                                    @{
                                        var akndto = Model.offerDto.OfferKnowledgeDtos.Where(x => x.KnowledgeLevel == (int)KnowledgeLevel.AdvantageOrOptionalKnowledge);
                                    }
                                    @foreach (var item in akndto)
                                    {
                                        <span>@item.KnowledgeDto.Name</span>
                                        if (akndto.Last() != item)
                                        {
                                            <span>,</span>
                                        }
                                    }


                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div>
                                        <h3> @Res.CompanyOffer.DrivingLicence</h3>

                                    </div>
                                </td>
                                <td>
                                    @{
                                        var licence = HomeController.GetLicenceType(Model.offerDto);
                                    }
                                    @foreach (var item in licence)
                                    {
                                        <span>@item</span>
                                        @if (licence.Last() != item)
                                        {
                                            <span>,</span>
                                        }
                                    }
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div>
                                        <h3>  @Res.CompanyProfile.NumberOfEmployee</h3>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td></td>
                                <td>
                                    <span id="numberEmploymentSpan">@Model.offerDto.CompanyDto.NumberOfEmployee</span>
                                </td>
                            </tr>
                            <tr>

                                <td>
                                    <br />
                                    <br />
                                    <br />
                                </td>
                            </tr>


                        </tbody>

                    </table>

                    




                </details>
            </div>
        </div>
        <div class="mg-top mg-left right-table">
            <details class="detailsbox" open>
                <table class="table  mg-top-lg">
                    <tbody>
                        <tr class="table-no-wrap">
                            <td class="flex-sm-basis">
                                <img width="20" height="33" src="/dsvg/SharedIcons/returnsicon">

                            </td>
                            <td>
                                <p>@Res.CompanyProfile.SendYourResume</p>
                            </td>
                        </tr>
                        <tr class="table-no-wrap">
                            <td class="flex-sm-basis">
                                <img width="20" height="33" src="/dsvg/SharedIcons/notepad">

                            </td>
                            <td><p>@Res.CompanyProfile.RecommandTheOffer</p></td>
                        </tr>
                        <tr class="table-no-wrap">
                            <td class="flex-sm-basis">
                                <img width="20" height="33" src="/dsvg/SharedIcons/facebookcolored">

                            </td>
                            <td><p>@Res.CompanyProfile.Recommandfacebook</p></td>
                        </tr>
                        <tr class="table-no-wrap">
                            <td class="flex-sm-basis">
                                <img width="20" height="33" src="/dsvg/SharedIcons/printer">

                            </td>
                            <td><p>@Res.CompanyProfile.Push</p></td>
                        </tr>
                        <tr class="table-no-wrap">
                            <td class="flex-sm-basis">
                                <img width="20" height="33" src="/dsvg/SharedIcons/locationmap">

                            </td>
                            <td><p>@Res.CompanyProfile.ShowPlaceOnMap</p></td>
                        </tr>
                        <tr class="table-no-wrap">
                            <td class="flex-sm-basis">
                                <img width="20" height="33" src="/dsvg/SharedIcons/stars">

                            </td>
                            <td><p>@Res.CompanyProfile.SaveOffer</p></td>
                        </tr>
                        <tr class="table-no-wrap">
                            <td class="flex-sm-basis">
                                <img width="20" height="33" src="/dsvg/SharedIcons/stars">

                            </td>
                            <td><p>@Res.CompanyProfile.ViewSavedJobs</p></td>
                        </tr>
                    </tbody>
                </table>
            </details>
        </div>
    </div>


        
    
</div>


<div id="documentModal" class="modalDialog">
    <div>
        <div class="Upload_document_line">
            <a onclick="closeModal('documentModal')" class="close">X</a>
            <h2>@Res.ApplicantProfile.ChoiceYourDocument </h2>
        </div>
        <div class="profile-flex-container">
            <br />
            @if (Model.applicantDocumentsDtos != null && Model.applicantDocumentsDtos.Count > 0)
            {
                @foreach (var item in Model.applicantDocumentsDtos)
                {
                    <div>
                        <input type="checkbox" id="@item.Id">
                        <span>@Res.ApplicantProfile.DocumentType :  @item.Type.ToString() ~ @item.CreateDate </span>
                        <br />
                        <br />
                        <br />
                    </div>
                }
            }

            else
            {
                @*<p>@Res.ApplicantProfile.ThereIsfDocumentUploaded</p>*@
            }


        </div>
        <br />
        <input type="button" class="btn-offer-Save" value="@Res.Cv.SendCv" />
    </div>
</div>
